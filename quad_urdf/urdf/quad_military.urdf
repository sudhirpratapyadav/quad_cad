  </link>

  <!-- front left lower leg -->
  <link name="front_left_lower_leg">
    <visual>
      <geometry>
        <mesh filename="../meshes/leg_lower_v3.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="lower_leg_color">
        <color rgba="0.1 0.1 0.1 1.0"/>
      </material>
    </visual>
  </link>

  <!-- front right lower leg -->
