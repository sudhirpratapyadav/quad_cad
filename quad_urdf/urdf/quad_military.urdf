    <visual>
      <geometry>
        <mesh filename="../meshes/leg_upper_with_motor_v1.5.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="leg_color">
        <color rgba="0.2 0.4 0.2 1.0"/>
      </material>
    </visual>
  </link>

  <!-- back left upper leg -->
  <link name="back_left_upper_leg">
    <visual>
      <geometry>
        <mesh filename="../meshes/leg_upper_with_motor_v1.5.stl" scale="0.001 0.001 0.001"/>
